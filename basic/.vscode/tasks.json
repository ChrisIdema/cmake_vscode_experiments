{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "cmake",
            "type": "shell",
            "options": {
                "cwd": "${config:cmake.buildDirectory}"
            },
            "command": "cmake",
            "args": [
                "--build",
                "${config:cmake.buildDirectory}",
                "--config",
                "Debug",
                "--target",
                "all",
                "-j",
                "10"
            ],
            "problemMatcher": [
                "$gcc"
            ]
        }       
    ]
}